##if [[ -z $DISPLAY ]] && [[ $(tty) = /dev/tty1 ]]; then exec startx; fi
source /home/zavarch/.alias 
##source /home/zavarch/.pam_environment
source /home/zavarch/.env
source "/home/zava/.homesick/repos/homesick/homesick.sh"
fpath=($HOME/.homesick/repos/homeshick/completions $fpath)
setopt PUSHD_MINUS
if [[ -f ${DIRSTACKFILE} ]]; then
     # Enabling NULL_GLOB via (N) weeds out any non-existing
     # directories from the saved dir-stack file.
     dirstacktmp=( ${(f)"$(< ${DIRSTACKFILE})"}(N) )
     for d in "${dirstacktmp[@]}"; do
         [[ -d ${d} ]] && [[ -r ${d} ]] && dirstack+=( ${d} )
     done
     builtin typeset -U dirstack
     builtin unset dirstacktmp d
 
     # "cd -" won't work after login by just setting $OLDPWD, so
     [[ -d $dirstack[1] ]] && cd -q $dirstack[1] && cd -q $OLDPWD
fi
prompt 'clint'
zstyle ':completion:*:cd:*' ignore-parents parent pwd
eval "$(direnv hook zsh)"
export $(dbus-launch)
#dbus-update-activation-environment --systemd  --verbose \ DBUS_SESSION_BUS_ADDRESS DISPLAY XAUTHORITY

